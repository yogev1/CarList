<p id="notice"><%= notice %></p>

<div class="container">
  <div class="row">
    <div class="col-md-4">
      <h1>Listing Cars</h1>
    </div>  
  </div>
</div>  

<div class="container">
  <div class="row">
    <% @cars.each do |car| %>
      <a class="thumbnail" href=" <%= car_path(car) %>"><div class="col-md-3"></div><img class="img-responsive"><%= image_tag car.image.url(:medium)%></a>
      <div class="col-md-1"></div>
    <% end %>
  </div>
</div>  
<br>

<div class="container">
  <div class="row">
    <p class="col-md-2"><%= link_to 'New Car', new_car_path %></p>
  </div>
</div> 
